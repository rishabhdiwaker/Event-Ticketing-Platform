          <div class="event-desc">

            <div class="event-date"></div>
            <div class="event-location"></div>
            <div class="event-price"></div>

          </div>
          <!-- ticket book -->
          <div class="book-tkt bookTicketButton">
            <input type="button" value="Book Ticket" class="card-btn" data-event-name="Event_1"
              data-event-date="30 November 2025" data-event-prize="249" data-event-venue="Pan India"
              onclick="redirectToBookingPage(this)" />
          </div>
        </div>
        <div class="card">
          <!-- img -->
          <div class="card-img">
            <img src="./images/arijit_singh.jpg" alt="" />
          </div>

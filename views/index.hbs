            <div class="event-date"></div>
            <div class="event-location"></div>
            <div class="event-price"></div>

          </div>
          <!-- ticket book -->
          <div class="book-tkt bookTicketButton">
            <input type="button" value="Book Ticket" class="card-btn" data-event-name=" Event_2"
              data-event-date="30 November 2024" data-event-venue="Delhi" data-event-prize="2500"
              onclick="redirectToBookingPage(this)" />
          </div>
        </div>
        <div class="card">
          <!-- img -->
          <div class="card-img">
            <img src="./images/stand_up.png" alt="" />
          </div>
          <!-- event -desc -->
          <div class="event-desc">

            <div class="event-date"></div>
            <div class="event-location"></div>
            <div class="event-price"></div>

          </div>
          <!-- ticket book -->
          <div class="book-tkt bookTicketButton">

          <div class="event-desc">

            <div class="event-date"></div>
            <div class="event-location"></div>
            <div class="event-price"></div>

          </div>
          <!-- ticket book -->
          <div class="book-tkt bookTicketButton">
            <input type="button" value="Book Ticket" class="card-btn" data-event-name="Event_5"
